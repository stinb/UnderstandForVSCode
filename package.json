{
	"name": "understand",
	"displayName": "Understand",
	"description": "The Software Developer's Multi-Tool",
	"version": "1.0.0",
	"publisher": "Scientific Toolworks, Inc.",
	"repository": {},
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Data Science",
		"Visualization"
	],
	"keywords": [
		"Understand",
		"Reference",
		"Definition",
		"Problems",
		"Analysis"
	],
	"icon": "logo/logoColor.png",
	"main": "extension.js",
	"activationEvents": [
		"onLanguage:ada",
		"onLanguage:assembly",
		"onLanguage:bat",
		"onLanguage:c",
		"onLanguage:cobol",
		"onLanguage:cpp",
		"onLanguage:csharp",
		"onLanguage:css",
		"onLanguage:cuda",
		"onLanguage:delphi",
		"onLanguage:fortran",
		"onLanguage:html",
		"onLanguage:java",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:jovial",
		"onLanguage:objective-c",
		"onLanguage:objective-cpp",
		"onLanguage:pascal",
		"onLanguage:perl",
		"onLanguage:php",
		"onLanguage:python",
		"onLanguage:python",
		"onLanguage:tcl",
		"onLanguage:text",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:vb",
		"onLanguage:verilog",
		"onLanguage:vhdl",
		"onLanguage:xml"
	],
	"contributes": {
		"configuration": {
			"title": "Understand",
			"properties": {
				"understand.executable.path": {
					"type": "string",
					"default": "",
					"description": "Search path(s) of the executable for the language server (default for Windows: %PATH%, default for other: /usr/bin:/usr/local/bin)"
				},
				"understand.files.watch": {
					"type": "string",
					"default": "**/*.{C,F,F90,H,TXT,a,ada,adb,ads,asm,bat,c,cbl,cc,cgi,cob,cpl,cpp,cpy,cs,css,cu,cuh,cxx,dfm,dpr,f,f03,f77,f90,f95,fmx,for,ftn,gpr,h,hh,hpp,htm,html,hxx,inl,java,jov,js,m,mm,pas,php,pl,plm,pm,py,s,sp,sql,tcl,ts,tsx,txt,upy,v,vb,vh,vhd,vhdl,xml}",
					"markdownDescription": "[Glob pattern](https://aka.ms/vscode-glob-patterns) of files to watch, to notify the server when they are changed/created/deleted"
				},
				"understand.protocol": {
					"type": "string",
					"default": "Local Socket",
					"enum": [
						"Local Socket",
						"TCP Socket"
					],
					"description": "Protocol to communicate with the language server"
				},
				"understand.protocols.localSocket.path": {
					"type": "string",
					"default": "",
					"description": "Path of local socket to communicate with the language server, where {uuid} is replaced with a randomly generated UUID\nDefault for Windows: \\\\.\\pipe\\userver-{uuid}\nDefault for other: /tmp/userver-{uuid}"
				},
				"understand.protocols.tcpSocket.port": {
					"type": "number",
					"default": 6789,
					"minimum": 0,
					"maximum": 65535,
					"description": "Port to communicate with the language server over TCP (integer 0 to 65535)"
				},
				"understand.trace.server": {
					"type": "string",
					"default": "Off",
					"enum": [
						"Off",
						"Messages",
						"Compact",
						"Verbose"
					],
					"description": "Log trace for communication with the language server"
				}
			}
		}
	},
	"dependencies": {
		"vscode-languageclient": "^9.0.0"
	},
	"devDependencies": {
		"esbuild": "^0.19.2"
	},
	"scripts": {
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=extension.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch"
	}
}
